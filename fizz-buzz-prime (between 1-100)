for number in range(1,101):
    if number % 3 == 0 and number % 5 == 0:
        print(number,"fizzbuzz")
        continue
    if number % 3 == 0:
        print (number, "fizz")
        continue
    if number % 5 == 0:
        print (number, "buzz")
        continue
    for x in range (2,number):
        if number % x == 0:
            print(number)
            break
    else:
        print (number, 'prime')
